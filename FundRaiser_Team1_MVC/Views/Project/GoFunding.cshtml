@model List<FundRaiser_Team1.Models.Package>

<h1> Choose Package</h1>
<br />
@foreach (var package in Model)
{
    <div class="card" style="width: 18rem; display:inline-block;">
        <div class="card-body">
            <h5 class="card-title">@package.PackageName</h5>
            <p class="card-text"></p>
        </div>
    </div>
}


